{
  "name": "@teamleader/ui",
  "description": "Teamleader UI library",
  "version": "0.1.5-beta",
  "author": "Teamleader <development@teamleader.eu>",
  "betterScripts": {
    "compile": {
      "command": "build-storybook -o dist",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:staging": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "staging",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "start-storybook",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/teamleadercrm/teamleader-ui/issues"
  },
  "contributors": [
    "Dries Decramer <dries.decramer@teamleader.eu>",
    "Nick Waelkens <nick.waelkens@teamleader.eu>",
    "Lowie Benoot <lowie.benoot@teamleader.eu>"
  ],
